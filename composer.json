{
  "name": "leonidas/leonidas",
  "description": "Framework for building WordPress plugins and themes",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "composer/installers": "^1.10",
    "dhii/i18n-interface": "^0.2.0",
    "guzzlehttp/psr7": "^1.6",
    "hassankhan/config": "^2.1",
    "league/container": "^3.3",
    "league/flysystem": "^2.0",
    "myclabs/php-enum": "^1.8",
    "psr/cache": "^1.0",
    "psr/container": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/link": "^1.0",
    "slim/slim": "^4.7",
    "twig/twig": "^2.0",
    "webtheory/guctilitybelt": "^0.2.0",
    "webtheory/html": "^0.2.0",
    "webtheory/saveyour": "^0.2.5"
  },
  "require-dev": {
    "10up/wp_mock": "0.4.2",
    "commerceguys/addressing": "^1.0",
    "egulias/email-validator": "^2.1",
    "fakerphp/faker": "^1.13",
    "filp/whoops": "^2.5",
    "friendsofphp/php-cs-fixer": "^2.18",
    "geocoder-php/google-maps-provider": "^4.4",
    "giggsey/libphonenumber-for-php": "^8.10",
    "humbug/php-scoper": "^0.14.0",
    "johnpbloch/wordpress": "^5.7",
    "lucatume/wp-browser": "3.0.5.1",
    "monolog/monolog": "^1.24",
    "php-http/guzzle6-adapter": "^1.0",
    "phpbench/phpbench": "@dev",
    "phpunit/phpunit": "^9.5",
    "psy/psysh": "^0.10.7",
    "spatie/phpunit-watcher": "^1.23",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/validator": "^4.3",
    "symfony/var-dumper": "^4.3",
    "wpackagist-plugin/advanced-wp-reset": "^1.1",
    "wpackagist-plugin/wp-reset": "^1.86"
  },
  "autoload": {
    "psr-4": {
      "Leonidas\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "boot/development.php"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test.admin": "@test --testsuite unit.admin",
    "test.admin.ui": "@test --testsuite unit.admin.ui",
    "watch": "phpunit-watcher watch",
    "check": "vendor/bin/phpcs",
    "fix": "vendor/bin/php-cs-fixer fix --verbose",
    "analyze": "vendor/bin/psalm",
    "bench": "vendor/bin/phpbench",
    "play": "vendor/bin/psych",
    "collect": "vendor/bin/var-dump-server",
    "build": [
      "@fix",
      "@check",
      "@test"
    ],
    "link": "bin/symlink-plugin",
    "wp-config": "cp -n wp-config-sample.php wp-config.php",
    "post-install-cmd": [
      "@link"
    ],
    "post-update-cmd": [
      "@link"
    ]
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wordpress/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "wordpress/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
