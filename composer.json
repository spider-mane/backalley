{
  "name": "backalley/backalley-core",

  "description": "Wordpress Admin Framework",

  "authors": [
    {
      "name": "Chris Williams",
      "email": "laserbeamvocalist@gmail.com"
    }
  ],

  "require": {
    "php": ">=7.0.0",
    "composer/installers": "v1.0.12",
    "egulias/email-validator": "^2.1",
    "illuminate/support": "^5.8",
    "jbzoo/utils": "^2.3",
    "league/fractal": "^0.18.0",
    "masterminds/html5": "^2.6",
    "moneyphp/money": "^3.2",
    "myclabs/deep-copy": "^1.8",
    "php-console/php-console": "^3.1",
    "respect/validation": "^1.1",
    "symfony/validator": "^4.3",
    "timber/timber": "^1.9",
    "twig/twig": "^2.0"
  },

  "autoload": {
    "psr-4": {
      "Backalley\\": "src/"
    },
    "files": ["backalley-core.php"]
  },

  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "filp/whoops": "^2.5",
    "phpbench/phpbench": "@dev",
    "symfony/var-dumper": "^4.3"
  }
}
