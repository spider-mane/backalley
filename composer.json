{
  "name": "leonidas/leonidas",
  "description": "Framework for building WordPress plugins and themes",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": "^8.0",
    "captainhook/captainhook": "^5.10",
    "composer/installers": "^1.10",
    "guzzlehttp/psr7": "^1.6",
    "hassankhan/config": "^2.1",
    "league/container": "^3.3",
    "league/flysystem": "^2.0",
    "myclabs/php-enum": "^1.8",
    "nesbot/carbon": "^2.54",
    "phpoption/phpoption": "^1.8",
    "phpunit/phpunit-selenium": "^9.0",
    "psr/cache": "^1.0",
    "psr/container": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/link": "^1.0",
    "psr/simple-cache": "^1.0",
    "symfony/filesystem": "^5.4",
    "symfony/finder": "^5.4",
    "symfony/polyfill-php80": "^1.23",
    "symfony/polyfill-php81": "^1.23",
    "symfony/string": "^5.4",
    "twig/twig": "^2.0",
    "webtheory/guctilitybelt": "^0.2.0",
    "webtheory/html": "^0.2.0",
    "webtheory/saveyour": "^0.2.5"
  },
  "require-dev": {
    "10up/wp_mock": "0.4.2",
    "aaemnnosttv/wp-sqlite-db": "^1.2",
    "brain/monkey": "^2.6",
    "brainmaestro/composer-git-hooks": "^2.8",
    "commerceguys/addressing": "^1.0",
    "egulias/email-validator": "^2.1",
    "fakerphp/faker": "^1.16",
    "filp/whoops": "^2.5",
    "friendsofphp/php-cs-fixer": "^2.18",
    "geocoder-php/google-maps-provider": "^4.4",
    "giggsey/libphonenumber-for-php": "^8.10",
    "koodimonni/composer-dropin-installer": "^1.3",
    "mockery/mockery": "^1.4",
    "monolog/monolog": "^2.0",
    "nunomaduro/collision": "^5.10",
    "opis/closure": "^3.6",
    "oscarotero/env": "^2.1",
    "php-http/guzzle6-adapter": "^1.0",
    "phpbench/phpbench": "@dev",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5",
    "psy/psysh": "^0.10",
    "slim/slim": "^4.7",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/console": "^5.3",
    "symfony/error-handler": "^5.3",
    "symfony/http-foundation": "^5.3",
    "symfony/http-kernel": "^5.3",
    "symfony/validator": "^5.3",
    "symfony/var-dumper": "^5.3",
    "symfony/yaml": "^5.3",
    "vlucas/phpdotenv": "^5.3",
    "wp-cli/wp-cli-bundle": "*",
    "wpackagist-plugin/wp-native-php-sessions": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Leonidas\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "WebTheory\\Debug\\": "packages/debug/src",
      "WebTheory\\WpDev\\": "packages/wp-dev/src",
      "WebTheory\\WpTest\\": "packages/wp-test/src",
      "WebTheory\\WpCliUtil\\": "packages/wp-cli-util/src"
    },
    "files": [
      "packages/wp-dev/src/wp-dmc.php"
    ]
  },
  "scripts": {
    "bench": "vendor/bin/phpbench",
    "check": "vendor/bin/phpcs",
    "collect": "vendor/bin/var-dump-server",
    "env": "cp -n .env.example .env",
    "fix": "vendor/bin/php-cs-fixer fix --verbose",
    "hooks": "vendor/bin/captainhook",
    "inspect": "vendor/bin/psalm",
    "link": "bin/symlink-plugin",
    "shell": "@wp shell",
    "test:acceptance": "vendor/bin/phpunit --testsuite acceptance",
    "test:functional": "vendor/bin/phpunit --testsuite functional",
    "test:integration": "vendor/bin/phpunit --testsuite integration",
    "test:unit": "vendor/bin/phpunit --testsuite unit",
    "test:wpunit": "vendor/bin/phpunit --testsuite wpunit",
    "watch": "phpunit-watcher watch",
    "wp": "vendor/bin/wp",
    "wp:refresh": [
      "@wp db export",
      "@wp db clean --yes",
      "@wp core install"
    ],
    "setup": [
      "@wp core download",
      "@link"
    ],
    "post-root-package-install": [
      "@env",
      "@setup"
    ]
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wordpress/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "wordpress/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ],
      "vendor/{$vendor}/{$name}/": [
        "type:wordpress-dropin"
      ]
    },
    "dropin-paths": {
      "wordpress/wp-content/": [
        "package:0aaemnnosttv/wp-sqlite-db:src/db.php"
      ]
    },
    "hooks": {}
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
