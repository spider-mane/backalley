{
  "name": "webtheory/leonidas",
  "description": "WordPress admin building library",
  "type": "wordpress-muplugin",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "composer/installers": "^1.7",
    "guzzlehttp/psr7": "^1.6",
    "hassankhan/config": "^2.1",
    "league/container": "^3.3",
    "monolog/monolog": "^1.24",
    "pimple/pimple": "^3.2",
    "twig/twig": "^2.0",
    "webtheory/guctilitybelt": "^0.2.0",
    "webtheory/html": "^0.1.0",
    "webtheory/saveyour": "^0.1.0"
  },
  "require-dev": {
    "commerceguys/addressing": "^1.0",
    "egulias/email-validator": "^2.1",
    "filp/whoops": "^2.5",
    "friendsofphp/php-cs-fixer": "^2.15",
    "geocoder-php/google-maps-provider": "^4.4",
    "giggsey/libphonenumber-for-php": "^8.10",
    "phpbench/phpbench": "@dev",
    "phpunit/phpunit": "^8.4",
    "symfony/thanks": "^1.1",
    "symfony/validator": "^4.3",
    "symfony/var-dumper": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "WebTheory\\Leonidas\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebTheory\\Leonidas\\Tests\\": "tests/"
    },
    "files": [
      "tests/acceptance/start.php"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
